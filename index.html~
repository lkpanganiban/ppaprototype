<script>
var map = L.map('map').setView([14.121, 121.5482], 7);
L.tileLayer('http://phlosmtiles.cloudapp.net/tiles/{z}/{x}/{y}.png', {
    maxZoom: 14,
    minZoom: 5
}).addTo(map);
</script>
